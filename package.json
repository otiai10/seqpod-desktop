{
  "name": "SeqPod",
  "version": "0.0.1",
  "description": "One step fastq processing and mutation calling",
  "main": "./app/main.js",
  "scripts": {
    "dev": "webpack --watch",
    "build": "webpack",
    "build-release": "API_URL=${SEQPOD_API_URL} NODE_ENV=production webpack",
    "start": "electron .",
    "clean": "rm -rf ./builds && mkdir ./builds",
    "package": "electron-packager . SeqPod --platform=${PLATFORM} --arg=x64 --version=${VERSION} --out=builds/${PLATFORM} --icon=assets/seqpod.icns --overwrite",
    "mac": "PLATFORM=darwin npm run package",
    "win": "PLATFORM=win32  npm run package && node ./scripts/windows-installer.js",
    "zip": "cd ./builds/darwin && zip -r ./SeqPod-darwin-x64.zip ./SeqPod-darwin-x64 && cd ../win32 && zip -r ./SeqPod-installer.zip ./SeqPod-installer",
    "deliver": "npm run clean && npm run mac && npm run win && npm run zip",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otiai10/fastpot.git"
  },
  "keywords": [
    "fastq",
    "bwa",
    "mutation-call"
  ],
  "author": "otiai10 <otiai10@gmail.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/otiai10/fastpot/issues"
  },
  "homepage": "https://github.com/otiai10/fastpot#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-riot": "^1.1.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chomex": "^1.0.10",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.9",
    "electron": "^1.8.1",
    "electron-packager": "^9.1.0",
    "electron-winstaller": "^2.6.4",
    "eslint": "^4.16.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "fs-extra": "^4.0.3",
    "history": "^4.6.3",
    "humanize-plus": "^1.8.2",
    "js-yaml": "^3.10.0",
    "node-sass": "^4.7.2",
    "photonkit": "^0.1.2",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.10.0"
  }
}
